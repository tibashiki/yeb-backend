<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxxx.server.mapper.EcMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xxxx.server.pojo.Ec">
        <id column="id" property="id" />
        <result column="ecType" property="ecType" />
        <result column="ecTypeName" property="ecTypeName" />
        <result column="ecPoint" property="ecPoint" />
        <result column="ecDate" property="ecDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ecType, ecTypeName, ecPoint, ecDate
    </sql>

</mapper>
